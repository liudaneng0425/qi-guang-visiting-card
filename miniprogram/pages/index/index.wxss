  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.banner {
  width: 750rpx;
  height: 417rpx;
}

.banner image {
  width: 100%;
  height: 417rpx;
}

.nameView {
  display: block;
  text-align: center;
}
.nameTxt {
  font-weight: bold;
  font-size: 60rpx;
  color: #FF0000;
}
.cell-group{
  margin: 20rpx 2rpx 2rpx 2rpx;
}
.cell-title{
  font-size: 40rpx;
}
.cell-title-phone{
  font-weight: bold;
  font-size: 40rpx;
}



.disable {
  color: #888;
}


